{
  "name": "smift",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "studio": "remotion studio src/index.ts",
    "render": "remotion render src/index.ts SaasIntro out/video.mp4",
    "preview": "remotion preview src/index.ts",
    "generate": "ts-node --compiler-options '{\"module\":\"commonjs\",\"jsx\":\"react-jsx\"}' src/pipeline/run.ts",
    "eval": "ts-node --compiler-options '{\"module\":\"commonjs\",\"jsx\":\"react-jsx\"}' src/pipeline/eval.ts",
    "eval:scraper": "ts-node --compiler-options '{\"module\":\"commonjs\",\"jsx\":\"react-jsx\"}' src/pipeline/eval-scraper.ts",
    "eval:packs": "ts-node --compiler-options '{\"module\":\"commonjs\",\"jsx\":\"react-jsx\"}' src/pipeline/eval-packs.ts",
    "eval:real": "ts-node --compiler-options '{\"module\":\"commonjs\",\"jsx\":\"react-jsx\"}' src/pipeline/eval-real.ts",
    "eval:real:smoke": "ts-node --compiler-options '{\"module\":\"commonjs\",\"jsx\":\"react-jsx\"}' src/pipeline/eval-real.ts --suite=smoke --max-script-attempts=1 --allow-low-quality --min-pass-rate=80 --min-pack-accuracy=80 --min-pack-comparable=8 --max-errors=2",
    "eval:customer": "ts-node --compiler-options '{\"module\":\"commonjs\",\"jsx\":\"react-jsx\"}' src/pipeline/eval-real.ts --suite=customer --max-script-attempts=1 --allow-low-quality --min-pass-rate=85 --min-pack-accuracy=85 --min-pack-comparable=20 --max-errors=4",
    "eval:customer:core": "ts-node --compiler-options '{\"module\":\"commonjs\",\"jsx\":\"react-jsx\"}' src/pipeline/eval-real.ts --suite=customer --segment=core-icp --max-script-attempts=1 --allow-low-quality --min-pass-rate=90 --min-pack-accuracy=90 --min-pack-comparable=16 --max-errors=2",
    "check:vision": "node scripts/check-vision-guard.js",
    "serve": "ts-node --compiler-options '{\"module\":\"commonjs\",\"jsx\":\"react-jsx\"}' src/server/serve.ts"
  },
  "dependencies": {
    "@remotion/bundler": "^4.0.421",
    "@remotion/cli": "^4.0.421",
    "@remotion/google-fonts": "^4.0.421",
    "@remotion/media-parser": "^4.0.421",
    "@remotion/media-utils": "^4.0.421",
    "@remotion/player": "^4.0.421",
    "@remotion/renderer": "^4.0.421",
    "@remotion/transitions": "^4.0.421",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "cheerio": "^1.2.0",
    "dotenv": "^17.2.4",
    "openai": "^6.21.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "remotion": "^4.0.421",
    "simple-icons": "^16.8.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
